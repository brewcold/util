{
  "name": "@brewcold/util",
  "version": "1.0.0",
  "description": "Javascript/Typescript util library for practice",
  "repository": "https://github.com/brewcoldblue/util",
  "keywords": [
    "javascript",
    "typescript",
    "react",
    "custom hooks",
    "hooks"
  ],
  "scripts": {
    "test": "pnpx jest --watchAll --verbose",
    "build": "tsc && pnpm bunchee --cwd react",
    "publish": "npm publish --access=public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./react": {
      "import": {
        "types": "./react/dist/index.d.mts",
        "default": "./react/dist/index.mjs"
      },
      "require": {
        "types": "./react/dist/index.d.ts",
        "default": "./react/dist/index.js"
      }
    }
  },
  "files": [
    "react/dist/**/*.{js,d.ts,mjs,d.mts}"
  ],
  "author": "Seungyoon Yu",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "axios": "^1.6.8",
    "bunchee": "^5.1.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "5.4.4",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^16.11.0 || ^17.0.0 || ^18.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 100,
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
